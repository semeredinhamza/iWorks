<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.employerTimeline_jsp</servlet-name>
        <servlet-class>org.apache.jsp.employerTimeline_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.loginPage_jsp</servlet-name>
        <servlet-class>org.apache.jsp.loginPage_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.studentSignUp_jsp</servlet-name>
        <servlet-class>org.apache.jsp.studentSignUp_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.EmployerRegister_jsp</servlet-name>
        <servlet-class>org.apache.jsp.EmployerRegister_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.studentTimelineLearning_jsp</servlet-name>
        <servlet-class>org.apache.jsp.studentTimelineLearning_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.studentTimeline_jsp</servlet-name>
        <servlet-class>org.apache.jsp.studentTimeline_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <servlet-class>org.apache.jsp.index_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.employerTimeline_jsp</servlet-name>
        <url-pattern>/employerTimeline.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.loginPage_jsp</servlet-name>
        <url-pattern>/loginPage.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.studentSignUp_jsp</servlet-name>
        <url-pattern>/studentSignUp.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.EmployerRegister_jsp</servlet-name>
        <url-pattern>/EmployerRegister.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.studentTimelineLearning_jsp</servlet-name>
        <url-pattern>/studentTimelineLearning.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.studentTimeline_jsp</servlet-name>
        <url-pattern>/studentTimeline.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <url-pattern>/index.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<welcome-file-list>
    <welcome-file>loginPage.jsp</welcome-file>
  </welcome-file-list>
</web-app>
